class Solution {
    public int prefixCount(String[] words, String pref) {
        int solution = 0;
        for (int i = 0; i < words.length; i ++){
            if (pref.length() > words[i].length()) {
                continue;
            }
            if (words[i].substring(0, pref.length()).equals(pref)) {
                solution++;
            }
        }
        return solution;
    }
}
